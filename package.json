{
  "name": "bytespace",
  "version": "0.0.1",
  "description": "Bytespace Website",
  "scripts": {
    "webpack-dev": "webpack-dev-server --watch --colors --progress",
    "build": "rimraf dist && webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "export NODE_ENV=production && node ./server/bin/www",
    "start--windows": "set NODE_ENV=production && node ./server/bin/www",
    "start-dev": "export NODE_ENV=development && node ./server/bin/www",
    "start-dev--windows": "set NODE_ENV=development && node ./server/bin/www",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack",
    "postinstall": "npm run build"
  },
  "keywords": [
    "Bytespace",
    "ExpressJS",
    "ReactJS",
    "Redux",
    "React hot loader",
    "React Router",
    "SCSS",
    "Webpack Devevelopment configuration",
    "Webpack Production configuration",
    "Airbnb Eslint",
    "mocha",
    "chai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VolkovGerman/bytespace.git"
  },
  "author": "OWU",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.16.1",
    "jquery": "^2.2.3",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "morgan": "^1.8.1",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.15.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "dateformat": "^2.0.0",
    "express": "^4.15.2",
    "normalize.css": "^7.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.5.2",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "serve-favicon": "^2.4.2",
    "serve-index": "^1.8.0",
    "serve-static": "^1.12.1",
    "swagger-jsdoc": "^1.9.2",
    "universal-cookie": "^2.0.5"
  }
}
